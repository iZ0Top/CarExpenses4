<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.info.InfoFragment">


    <TextView
        android:id="@+id/info_year"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="2001"
        android:gravity="end"
        app:layout_constraintEnd_toStartOf="@id/info_fuel_type"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/info_fuel_type" />

    <TextView
        android:id="@+id/info_fuel_type"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="GAS/LPG"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toStartOf="@id/info_engine_volume"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/info_year"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/info_engine_volume"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:text="1.4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/info_fuel_type"
        app:layout_constraintTop_toTopOf="@+id/info_fuel_type" />

    <TextView
        android:id="@+id/info_win_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="AAA 111 222 333 444 "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/info_fuel_type" />

    <TextView
        android:id="@+id/info_i_drive_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="I drive:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/info_win_code" />

    <TextView
        android:id="@+id/info_i_drive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:text="10 year 11 month 12 days"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/info_i_drive_text" />

    <TextView
        android:id="@+id/info_i_drove_text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="4dp"
        android:gravity="end"
        android:text="I drove:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/info_i_drove"
        app:layout_constraintTop_toBottomOf="@+id/info_i_drive" />

    <TextView
        android:id="@+id/info_i_drove"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="0 km"
        android:layout_marginStart="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.575"
        app:layout_constraintStart_toEndOf="@+id/info_i_drove_text"
        app:layout_constraintTop_toTopOf="@+id/info_i_drove_text" />

    <TextView
        android:id="@+id/info_i_spent_text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="4dp"
        android:gravity="end"
        android:text="I spent:"
        app:layout_constraintEnd_toStartOf="@id/info_i_spent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/info_i_drove_text" />

    <TextView
        android:id="@+id/info_i_spent"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="0.0 uah"
        android:layout_marginStart="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.894"
        app:layout_constraintStart_toEndOf="@+id/info_i_spent_text"
        app:layout_constraintTop_toTopOf="@+id/info_i_spent_text" />
    <TextView
        android:id="@+id/info_temp_string"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="info"
        android:gravity="center_horizontal"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@id/info_i_spent_text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

</androidx.constraintlayout.widget.ConstraintLayout>